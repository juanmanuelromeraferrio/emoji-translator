generator client {
  provider   = "prisma-client-js"
  engineType = "library"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Emoji {
  word   String @id
  emoji  String
  source String

  @@index([word])
}

model RecentTranslation {
  id         String   @id(map: "Recents_pkey") @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  word       String?
  emoji      String?
  created_at DateTime @default(now()) @db.Timestamptz(6)

  @@index([created_at])
}
